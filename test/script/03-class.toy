class Num {
    let val;

    constructor(this, x) {
        this.val = x;
    }

    function add(this, y) {
        this.val += y;
    }
}

let a = new Num(1);
let b = new Num(2);
a.add(3);
b.add(5);
print(a.val + b.val);   // 11